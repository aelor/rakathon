<div class="row">
  <div class="col-md-4 offset-md-4 white-panel">
<p id="notice"><%= notice %></p>

<h1>Owners</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Mobile</th>
      <th>No of parking</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parking_spaces.each do |owner| %>
      <tr>
        <td><%= owner.name %></td>
        <td><%= owner.address %></td>
        <td><%= owner.mobile %></td>
        <td><%= owner.no_of_parking %></td>
        <td><%= owner.price %></td>
        <td><%= link_to 'Show', owner %></td>
        <td><%= link_to 'Edit', edit_parking_space_path(owner) %></td>
        <td><%= link_to 'Destroy', owner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>
</div>
<%= link_to 'New Owner', new_owner_path %>
